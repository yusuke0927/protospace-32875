<%= form_with model: @prototype, local: true do |f|%>
  <div class="field">
    <%= f.label :"プロトタイプの名称" %><br />
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <%= f.label :"キャッチコピー" %><br />
    <%= f.text_area :catch_copy %>
  </div>

  <div class="field">
    <%= f.label :"コンセプト" %><br />
    <%= f.text_area :concept %>
  </div>

  <div class="field">
    <%= f.label :"プロトタイプの画像" %><br />
    <%= f.file_field :image %>
  </div>

  <div class="actions">
    <%= f.submit "保存する" %>
  </div>
<% end %>